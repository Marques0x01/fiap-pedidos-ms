{
  "name": "hello_world",
  "version": "1.0.0",
  "description": "hello world sample for NodeJS",
  "main": "app.js",
  "repository": "https://github.com/awslabs/aws-sam-cli/tree/develop/samcli/local/init/templates/cookiecutter-aws-sam-hello-nodejs",
  "author": "SAM CLI",
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^29.5.12",
    "axios": "^1.6.8",
    "express": "^4.19.2",
    "jest": "^29.7.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "uuid": "^9.0.1"
  },
  "scripts": {
    "test": "jest --coverage --ci --reporters=default --reporters=jest-junit"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/config",
      "/src/domain",
      "/src/http",
      "/src/repository"
    ]
  },
  "jest-junit": {
    "suiteName": "Test Suite",
    "output": "./junit.xml"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 2
  },
  "devDependencies": {
    "babel-plugin-add-module-exports": "^1.0.4",
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.1",
    "cucumber": "^6.0.7",
    "jest-junit": "^16.0.0",
    "mocha": "^10.2.0"
  }
}
